{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.sendwise.nl/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "sendwise.nl"
        }
      ]
    }
  ]
}

