    <style id="sendwise-blur-prices">
      /* Basis: prijs in blauw, maar zo sterk geblurred dat je het niet kunt lezen */
      .text-primary.price {
        position: relative;
        color: #0063ff !important; /* originele Sendwise-blauw */
        filter: blur(6.5px);        /* sterke blur -> tekst niet meer leesbaar */
        display: inline-block;
        user-select: none;          /* tekst niet selecteerbaar */
        -webkit-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        pointer-events: none;       /* voorkomt interactie met de tekst zelf */
      }

      /* Fancy blauwe glow overlay */
      .text-primary.price::after {
        content: "";
        position: absolute;
        inset: -6px;
        background: radial-gradient(circle at center,
          rgba(0, 99, 255, 0.55) 0%,
          rgba(0, 99, 255, 0.25) 45%,
          rgba(0, 99, 255, 0) 85%
        );
        pointer-events: none;
        border-radius: 50%;
        animation: pulseBlur 4s infinite ease-in-out;
      }

      /* Hover: nog intensere blur + fellere glow */
      .text-primary.price:hover {
        filter: blur(7.5px);
      }
      .text-primary.price:hover::after {
        background: radial-gradient(circle at center,
          rgba(0, 99, 255, 0.7) 0%,
          rgba(0, 99, 255, 0.35) 45%,
          rgba(0, 99, 255, 0) 85%
        );
      }

      /* Pulse animatie voor fancy dynamiek */
      @keyframes pulseBlur {
        0%, 100% { opacity: 0.6; transform: scale(1); }
        50% { opacity: 0.9; transform: scale(1.1); }
      }
