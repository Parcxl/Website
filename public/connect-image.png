        function stripTemplateHeader(ctx){
          var nodes = (ctx || document).querySelectorAll('.elementor-templates-modal__header__items-area');
          nodes.forEach(function(el){
            var txt = (el.textContent || '').trim();
            var html = (el.innerHTML || '');
            // Check op Underscore/Elementor templating markers
            if (txt.indexOf('<#') !== -1 || html.indexOf('&lt;#') !== -1 || txt.indexOf('{{{') !== -1) {
              el.remove(); // haal het hele blok weg
            }
          });

          // Extra defensief: als losse tekstnodes met templating zichtbaar zijn, verwijder hun parent
          var walker = document.createTreeWalker(document.body, NodeFilter.SHOW_TEXT, null);
          var n;
          while ((n = walker.nextNode())) {
            var v = n.nodeValue || '';
            if (v.indexOf('<#') !== -1 || v.indexOf('{{{') !== -1 || v.indexOf('&lt;#') !== -1) {
              if (n.parentElement) n.parentElement.remove();
            }
          }
        }

        // Init en blijf opletten op dynamische injecties
        stripTemplateHeader();
        new MutationObserver(function(muts){
          for (var i=0;i<muts.length;i++){
            var m = muts[i];
            if (m.type === 'childList' || m.type === 'attributes') {
