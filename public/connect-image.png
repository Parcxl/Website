        function stripIcons(ctx){
          (ctx || document).querySelectorAll('nav.main-nav .nav-list .nav-item .item-link i.icon-polygon')
            .forEach(function(el){ el.remove(); });
        }

        // 2) Behoud basiskleur: lees computed color en zet die inline (overrulet elke hover CSS)
        function lockBaseColor(ctx){
          var links = (ctx || document).querySelectorAll('nav.main-nav .nav-list .nav-item > a.item-link');
          links.forEach(function(a){
            // Haal de huidige (non-hover) kleur van de link of de span op
            var span = a.querySelector('span');
            var colorEl = span || a;
            var c = window.getComputedStyle(colorEl).color;
            // Zet kleur inline op zowel <a> als <span> zodat geen hover CSS kan winnen
            a.style.setProperty('color', c, 'important');
            if(span){ span.style.setProperty('color', c, 'important'); }
          });
        }

        function runAll(ctx){
          stripIcons(ctx);
          lockBaseColor(ctx);
        }

        // Init
        runAll();

        // Re-apply bij veranderingen (mega menu's/Elementor wijzigen vaak DOM live)
        var nav = document.querySelector('nav.main-nav') || document.body;
